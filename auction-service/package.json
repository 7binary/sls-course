    "@types/aws-sdk": "^2.7.0",
    "@types/http-errors": "^1.8.0",
    "@types/node": "^14.14.37",
    "@types/uuid": "^8.3.0",
    "babel-loader": "^8.2.2",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "serverless-pseudo-parameters": "^2.5.0",
